<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="800dp"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="0dp"
        android:layout_height="350dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/scroll" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="Items"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="@+id/imageView8" />

    <TextView
        android:id="@+id/txtCloseBattleItemDialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="93dp"
        android:layout_marginBottom="8dp"
        android:text="TUTUP"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView8" />

    <ImageView
        android:id="@+id/imgBandageBattleItemDialog"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/rounded_corners"
        android:backgroundTint="@color/wood_brown_dark1"
        android:padding="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgJamuBattleItemDialog"
        app:layout_constraintEnd_toStartOf="@+id/imgJamuBattleItemDialog"
        app:layout_constraintTop_toTopOf="@+id/imgJamuBattleItemDialog"
        app:srcCompat="@drawable/battle_bandage" />

    <ImageView
        android:id="@+id/imgHourglassBattleItemDialog"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginStart="32dp"
        android:background="@drawable/rounded_corners"
        android:backgroundTint="@color/wood_brown_dark1"
        android:padding="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgJamuBattleItemDialog"
        app:layout_constraintStart_toEndOf="@+id/imgJamuBattleItemDialog"
        app:layout_constraintTop_toTopOf="@+id/imgJamuBattleItemDialog"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/battle_hourglass" />

    <ImageView
        android:id="@+id/imgJamuBattleItemDialog"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/rounded_corners"
        android:backgroundTint="@color/wood_brown_dark1"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:srcCompat="@drawable/battle_jamu" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Perban"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/imgBandageBattleItemDialog"
        app:layout_constraintEnd_toEndOf="@+id/imgBandageBattleItemDialog"
        app:layout_constraintStart_toStartOf="@+id/imgBandageBattleItemDialog" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Jamu"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/imgJamuBattleItemDialog"
        app:layout_constraintEnd_toEndOf="@+id/imgJamuBattleItemDialog"
        app:layout_constraintStart_toStartOf="@+id/imgJamuBattleItemDialog" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Jam Pasir"
        android:textColor="#040404"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/imgHourglassBattleItemDialog"
        app:layout_constraintEnd_toEndOf="@+id/imgHourglassBattleItemDialog"
        app:layout_constraintStart_toStartOf="@+id/imgHourglassBattleItemDialog" />

    <TextView
        android:id="@+id/txtBandageAmountBattleItemDialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="0"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imgBandageBattleItemDialog"
        app:layout_constraintStart_toStartOf="@+id/imgBandageBattleItemDialog"
        app:layout_constraintTop_toBottomOf="@+id/imgBandageBattleItemDialog" />

    <TextView
        android:id="@+id/txtJamuAmountBattleItemDialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="0"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imgJamuBattleItemDialog"
        app:layout_constraintStart_toStartOf="@+id/imgJamuBattleItemDialog"
        app:layout_constraintTop_toBottomOf="@+id/imgJamuBattleItemDialog" />

    <TextView
        android:id="@+id/txtHourglassAmountBattleItemDialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="0"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imgHourglassBattleItemDialog"
        app:layout_constraintStart_toStartOf="@+id/imgHourglassBattleItemDialog"
        app:layout_constraintTop_toBottomOf="@+id/imgHourglassBattleItemDialog" />

</androidx.constraintlayout.widget.ConstraintLayout>